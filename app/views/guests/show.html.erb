<div class="page-header">
  <h3>Guest #<%= @guest.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @guest.user_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/guests" class="btn btn-primary">
        Back
      </a>
      <a href="/guests/<%= @guest.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_guest/<%= @guest.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>

<!-- A Guest has many msgrecievers -->

<div class="row">
  <div class="col-md-12">
    <ul class="list-group">
      <li class="list-group-item list-group-item-info">
        Msgrecievers
      </li>

      <% @guest.msgrecievers.each do |msgreciever| %>
        <li class="list-group-item">
          <a href="/msgrecievers/<%= msgreciever.id %>">
            <%= msgreciever.host_id %>
          </a>
          <div class="btn-group btn-group-xs pull-right">
            <a href="/msgrecievers/<%= msgreciever.id %>" class="btn btn-primary">
              <i class="fa fa-search-plus"></i>
            </a>
            <a href="/msgrecievers/<%= msgreciever.id %>/edit" class="btn btn-warning">
              <i class="fa fa-edit"></i>
            </a>
            <a href="/delete_msgreciever/<%= msgreciever.id %>" class="btn btn-danger" rel="nofollow">
              <i class="fa fa-trash-o"></i>
            </a>
          </div>
        </li>
      <% end %>

      <li class="list-group-item">
        <form action="/create_msgreciever" method="post">
      <!-- Hidden input for authenticity token to protect from forgery -->
      <input name="authenticity_token" type="hidden" value="<%= form_authenticity_token %>">

      <!-- Label and input for host_id -->
      <div class="form-group">
        <label for="host_id" class="control-label">
          Host
        </label>

        <%= select_tag(:host_id, options_from_collection_for_select(Host.all, :id, :user_id), :class => "form-control") %>
      </div>

      <!-- Label and input for guest_id -->
      <input type="hidden" name="guest_id" value="<%= @guest.id %>">

      <button class="btn btn-block btn-success">
        Create Msgreciever
      </button>
    </form>
      </li>
    </ul>
  </div>
</div>

